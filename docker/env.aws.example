# AWS environment variables (template)
NODE_ENV=production
IS_DEBUG_MODE=false
VITE_IS_DEBUG_MODE=false
SIGN_IN_PREFILLED=false
APP_SECRET=__REPLACE_WITH_RANDOM_SECRET__

EXTERNAL_HOST=your-domain-or-ip
FRONTEND_PORT=8866
BACKEND_PORT=8867
POSTGRES_PORT=5432
REDIS_PORT=6379
NODE_PORT=8867

FRONTEND_URL=http://your-domain-or-ip:8866
BACKEND_URL=http://your-domain-or-ip:8867
SERVER_URL=http://your-domain-or-ip:8867
BACKEND_PUBLIC_URL=http://your-domain-or-ip:8867
FRONTEND_IMAGE_VERSION=f_placeholder_image_version
BACKEND_IMAGE_VERSION=b_placeholder_image_version

POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=default
POSTGRES_URL=postgres://postgres:postgres@postgres:5432/default
PG_DATABASE_URL=postgres://postgres:postgres@postgres:5432/default
REDIS_URL=redis://redis:6379

IS_MULTIWORKSPACE_ENABLED=false
IS_WORKSPACE_CREATION_LIMITED_TO_SERVER_ADMINS=true
DEFAULT_SUBDOMAIN=app

TIMELINE_RETENTION_DAYS=180

IS_BILLING_ENABLED=false
BILLING_STRIPE_API_KEY=__REPLACE_WITH_STRIPE_KEY__
BILLING_STRIPE_BASE_PLAN_PRODUCT_ID=__REPLACE_WITH_STRIPE_PRODUCT_ID__

EMAIL_DRIVER=SMTP
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_SMTP_USER=__REPLACE_WITH_EMAIL_USER__
EMAIL_SMTP_PASSWORD=__REPLACE_WITH_EMAIL_PASSWORD__
EMAIL_FROM_ADDRESS=__REPLACE_WITH_EMAIL_FROM_ADDRESS__
EMAIL_FROM_NAME=Y-CRM

MESSAGING_PROVIDER_GMAIL_ENABLED=true
CALENDAR_PROVIDER_GOOGLE_ENABLED=true
AUTH_GOOGLE_CLIENT_ID=__REPLACE_WITH_GOOGLE_CLIENT_ID__
AUTH_GOOGLE_CLIENT_SECRET=__REPLACE_WITH_GOOGLE_CLIENT_SECRET__
AUTH_GOOGLE_CALLBACK_URL=http://your-domain-or-ip:8867/auth/google/redirect
AUTH_GOOGLE_APIS_CALLBACK_URL=http://your-domain-or-ip:8867/auth/google-apis/get-access-token

OPENAI_API_KEY=__REPLACE_WITH_OPENAI_API_KEY__

LINE_CHANNEL_ACCESS_TOKEN=__REPLACE_WITH_LINE_CHANNEL_ACCESS_TOKEN__
LINE_CHANNEL_SECRET=__REPLACE_WITH_LINE_CHANNEL_SECRET__
